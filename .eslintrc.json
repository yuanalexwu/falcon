{
  "parser": "babel-eslint",
  "extends": ["standard", "standard-react"],
  "rules": {
    "react/jsx-space-before-closing": [2, "always"],
    "react/prop-types": [0],
    "no-debugger": [0],
    "comma-dangle": [0],
    "camelcase": [0],
    "valid-typeof": [0],
    "standard/no-callback-literal": [0]
  }
}
